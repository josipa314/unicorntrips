<h3>Look for your next cool activity to experience:</h3>

{{#each activities}}
  <section class="activities-list">
    <h2>{{this.title}}</h2>
    <h4>by {{this.agency.name}}</h4>

    <img id="imgActivities" src={{imageFile}} alt={{this.title}} />
    <br>

    <a href="/activities/{{this._id}}">More details</a>
    <hr>
  </section>
{{/each}}